public class reverse {   
    public void main(String[] args) 
    { 
        String s[] = "the sky is blue".split(" "); 
        String rev = ""; 
        for (int i = s.length - 1; i >= 0; i--) { 
            rev += s[i] + " "; 
        } 
        System.out.println(ans.substring(0, rev.length() - 1)); 
    } 
} 
